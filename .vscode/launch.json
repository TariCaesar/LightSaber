    "version": "0.2.0",
    "configurations": [
        {
            "name": "(arm-gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "targetArchitecture": "arm",
            "program": "${workspaceRoot}/Debug/out.elf",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/Dev/Gcc-arm/bin/arm-none-eabi-gdb.exe",
            "miDebuggerArgs": "--eval-command \"target remote :5200\"",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "util"
        }
    ]
}